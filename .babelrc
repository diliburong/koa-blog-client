{
    "presets": [
        ["vue-app"],
        [
            "es2015",
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ],
        [
            "import",
            {
                "libraryName": "muse-ui",
                "libraryDirectory": "lib"
            }
        ]
    ]
}
